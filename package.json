    "demo": "npx sass demo/style.sass demo/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeKarmaDev/fo-mason.git"
  },
  "keywords": ["masonry", "grid", "css", "sass", "mason"],
  "author": "Sean Ferney sean@codekarma.dev",
