{
  "name": "fo-mason",
  "version": "1.0.0",
  "description": "fo-mason is a css-grid based masonry layout library built with sass. No javascript required!",
  "main": "index.scss",
  "directories": {
    "test": "test"
  },
  "files": [
    "mason/*.sass",
    "mason/*.scss",
    "index.scss"
  ],
  "scripts": {
